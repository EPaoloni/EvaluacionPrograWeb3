@model EvaluacionAprendizaje.Models.Partials.Competidor

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Alta Competidor</h2>

<form class="container" method="get" action="./RealizarAltaCompetidor">
    <label for="nombre">Nombre: </label>
    @Html.EditorFor(o => o.nombre)
    @Html.ValidationMessageFor(o => o.nombre)
    <button class="btn btn-primary">Dar de Alta</button>
</form>

<div class="container">
    <a class="btn btn-danger" href="./">Cancelar</a>
</div>
